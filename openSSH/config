
# Home-mini-pc (jellyfin server)
Host minipc
	HostName local ip/isp DNS
	User login as user
	IdentityFile ~/.ssh/ssh_key
	IdentitiesOnly yes

# Home-mini-pc (git server)
Host gitserver
	HostName brittjtb.hopto.org
	Port router port/default 22
	User login as this user
	IdentityFile ~/.ssh/ssh_key
	IdentitiesOnly yes
